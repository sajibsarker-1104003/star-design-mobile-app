{
	"name": "start-design-mobile-app-be",
	"version": "1.0.0",
	"author": "star-design",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "npx rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"prisma:generate": "prisma generate",
		"prisma:migrate-dev": "prisma migrate dev",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"pretest:e2e": "dotenv -e .env.test -- prisma migrate reset --force",
		"test:e2e": "dotenv -e .env.test -- jest -i --no-cache --config ./test/jest-e2e.json --verbose --forceExit",
		"test:cov-e2e": "dotenv -e .env.test -- jest --coverage --config ./test/jest-e2e.json",
		"prisma:dev:migrate": "prisma migrate dev",
		"prisma:deploy": "prisma migrate deploy"
	},
	"dependencies": {
		"@casl/ability": "^6.5.0",
		"@nestjs-modules/mailer": "^2.0.2",
		"@nestjs/axios": "^4.0.0",
		"@nestjs/cache-manager": "^3.0.1",
		"@nestjs/common": "^10.0.5",
		"@nestjs/config": "^3.0.0",
		"@nestjs/core": "^10.0.5",
		"@nestjs/jwt": "^10.1.0",
		"@nestjs/mapped-types": "^2.0.2",
		"@nestjs/passport": "^11.0.5",
		"@nestjs/platform-express": "^10.0.5",
		"@nestjs/schedule": "^3.0.1",
		"@nestjs/swagger": "^7.1.1",
		"@nestjs/terminus": "^11.0.0",
		"@prisma/client": "4.16.2",
		"@types/passport-facebook": "^3.0.3",
		"argon2": "^0.30.3",
		"aws-sdk": "^2.1629.0",
		"axios": "^1.8.2",
		"cache-manager-ioredis-yet": "^2.1.2",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"connect-redis": "9",
		"crypto": "^1.0.1",
		"csv-writer": "^1.6.0",
		"dotenv": "^17.0.1",
		"express-session": "^1.18.1",
		"ioredis": "^5.6.1",
		"jsonwebtoken": "^9.0.1",
		"jwt-decode": "^3.1.2",
		"minio": "^8.0.5",
		"nestjs-cls": "^3.5.0",
		"nodemailer": "^7.0.5",
		"openai": "^4.52.0",
		"papaparse": "^5.4.1",
		"passport": "^0.7.0",
		"passport-facebook": "^3.0.0",
		"passport-google-oauth20": "^2.0.0",
		"passport-jwt": "^4.0.1",
		"pdfmake": "^0.2.7",
		"redis": "^5.5.6",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^5.0.1",
		"rxjs": "^7.8.1",
		"stripe": "^13.7.0"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.1.8",
		"@nestjs/schematics": "^10.0.1",
		"@nestjs/testing": "^10.0.5",
		"@types/cron": "^2.0.1",
		"@types/express": "^4.17.17",
		"@types/jest": "29.5.3",
		"@types/multer": "^1.4.7",
		"@types/node": "^20.11.25",
		"@types/papaparse": "^5.3.7",
		"@types/passport-google-oauth20": "^2.0.16",
		"@types/passport-jwt": "^3.0.9",
		"@types/pdfmake": "^0.2.2",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"dotenv-cli": "^7.2.1",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^5.0.0",
		"jest": "^29.6.1",
		"prettier": "^3.0.0",
		"prisma": "^4.16.2",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.1",
		"ts-loader": "^9.4.4",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.3.3",
		"webpack": "^5.0.0"
	},
	"release": {
		"branches": [
			"main"
		],
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/npm",
			[
				"@semantic-release/git",
				{
					"assets": [
						"./"
					],
					"message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
				}
			],
			[
				"@semantic-release/github",
				{
					"successComment": false,
					"failComment": false
				}
			]
		]
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "./",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "./coverage/unit",
		"testEnvironment": "node"
	},
	"packageManager": "yarn@4.9.0"
}
